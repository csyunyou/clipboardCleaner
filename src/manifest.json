{
  "name": "ClipBoardCleaner",
  "manifest_version": 2,
  "version": "1.0",
  "description": "To remove advertisements created by juejin when copy option is made",
  "browser_action":
  {
    "default_icon": "avatar.png"
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["main.js"],
    "run_at": "document_idle"
  }]
}